{
  "name": "npm-path",
  "version": "2.0.3",
  "description": "Get a PATH with all executables available to npm scripts.",
  "main": "index.js",
  "bin": {
    "npm-path": "bin/npm-path"
  },
  "scripts": {
    "build": "babel src --out-dir .",
    "prepublish": "npm run build",
    "tests-only": "tape test -r airbnb-js-shims -r ./test/_babel | faucet",
    "pretest": "standard src/* test/* bin/*",
    "test": "npm run build && npm run tests-only"
  },
  "author": "Tim Oxley",
  "license": "MIT",
  "engines": {
    "node": ">=4.2.0"
  },
  "devDependencies": {
    "airbnb-js-shims": "^1.4.0",
    "babel-cli": "^6.26.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-register": "^6.26.0",
    "faucet": "0.0.1",
    "npm": "^3.10.10",
    "standard": "^7.1.2",
    "tape": "^4.8.0"
  },
  "dependencies": {
    "which": "^1.2.10"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timoxley/npm-path.git"
  },
  "keywords": [
    "npm",
    "run",
    "executable"
  ],
  "bugs": {
    "url": "https://github.com/timoxley/npm-path/issues"
  },
  "homepage": "https://github.com/timoxley/npm-path"
}
